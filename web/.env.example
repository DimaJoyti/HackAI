# Firebase Configuration - Development
NEXT_PUBLIC_FIREBASE_API_KEY_DEV=your_dev_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN_DEV=hackai-dev.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID_DEV=hackai-dev
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET_DEV=hackai-dev.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID_DEV=your_dev_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID_DEV=your_dev_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID_DEV=your_dev_measurement_id

# Firebase Configuration - Staging
NEXT_PUBLIC_FIREBASE_API_KEY_STAGING=your_staging_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN_STAGING=hackai-staging.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID_STAGING=hackai-staging
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET_STAGING=hackai-staging.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID_STAGING=your_staging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID_STAGING=your_staging_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID_STAGING=your_staging_measurement_id

# Firebase Configuration - Production
NEXT_PUBLIC_FIREBASE_API_KEY_PROD=your_prod_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN_PROD=hackai-prod.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID_PROD=hackai-prod
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET_PROD=hackai-prod.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID_PROD=your_prod_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID_PROD=your_prod_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID_PROD=your_prod_measurement_id

# Firebase Emulator (Development only)
NEXT_PUBLIC_USE_FIREBASE_EMULATOR=false

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_FIREBASE_AUTH_API_URL=http://localhost:8081

# Environment
NODE_ENV=development
